{
    "name": "mixsnoep/laravel-pdf-print",
    "description": "A Laravel package for PDF generation using headless Chrome. Zero external dependencies.",
    "keywords": ["laravel", "pdf", "chrome", "headless", "print"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mixsnoep",
            "email": "info@mixsnoep.nl"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/view": "^11.0|^12.0",
        "illuminate/contracts": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Mixsnoep\\PdfPrint\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mixsnoep\\PdfPrint\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mixsnoep\\PdfPrint\\PdfServiceProvider"
            ],
            "aliases": {
                "Pdf": "Mixsnoep\\PdfPrint\\Facades\\Pdf"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
